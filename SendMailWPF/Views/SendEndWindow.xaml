<Window x:Class="SendMailWPF.Views.SendEndWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SendMailWPF.Views"
        mc:Ignorable="d"
        Title="SendEndWindow" Height="120" Width="400"
        ResizeMode="NoResize"
        >
    <StackPanel Orientation="Vertical">
        <Label Name="lblSendEnd"
               Height="auto" Width="auto"
               VerticalAlignment="Center"
               HorizontalAlignment="Center"
               FontSize="20"
               >
            
        </Label>
        <Button Height="25" Width="100" Margin="0,10,0,0" Click="Button_Click">
            <TextBlock Text="ОК" FontSize="15"/>
        </Button>

    </StackPanel>
</Window>
